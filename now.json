{
    "version": 2,
    "alias": "ghmidi.now.sh",
    "routes": [
        {
            "src": "/api/(?<username>[^/]+)\/?",
            "dest": "/api.js?username=$username"
        }
    ],
    "builds": [
        {
            "src": "./api.js",
            "use": "@now/node"
        },
        {
            "src": "./index.html",
            "use": "@now/html-minifier"
        },
        {
            "src": "./front/*",
            "use": "@now/static"
        }
    ]
}
